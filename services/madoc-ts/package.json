    "start-local": "MADOC_KEY_PATH=../../var/certs/ JWT_REQUEST_DIR=./service-jwts JWT_RESPONSE_DIR=../../var/jwt node ./lib/index.js",
    "dev": "NODE_ENV=development pm2 start ecosystem.config.js --watch --no-daemon",
    "start": "concurrently yarn:watch yarn:frontend",
    "storybook": "start-storybook -p 6500",
    "watch": "tsc -p . --watch",
    "test": "jest",
    "build": "NODE_ENV=production yarn generate-schema && tsc -p . && yarn build:frontend",
    "generate-migration": "node migrate.js create",
    "generate-schema": "node generate-schemas.js",
    "build:stats": "NODE_ENV=production webpack --progress --json --profile > stats.json",
    "build:frontend": "NODE_ENV=production webpack",
    "frontend": "SKIP_HOT_RELOAD=true webpack --watch",
    "link:model-editor": "yarn link \"@capture-models/editor\"",
    "unlink:model-editor": "yarn unlink \"@capture-models/editor\" && yarn install --force",
    "link:model-helpers": "yarn link \"@capture-models/helpers\"",
    "unlink:model-helpers": "yarn unlink \"@capture-models/helpers\" && yarn install --force",
    "extract:api": "tsc -p ./tsconfig.types.json"
  },
